<div class="container">
<h1>Registrar miembro</h1>
  <div class="row">
<%= form_for (@member) do |f| %>
    <div class="input-field">
      <i class="material-icons prefix">account_circle</i>
      <%= f.text_field :name, class:"validate", :id=>"first_name", class:"validate"%>
      <label for="name">First Name</label>

    </div>
      <div class="input-field">
        <i class="material-icons prefix">account_circle</i>
      <%= f.text_field :last_name, :id=>"last_name", class:"validate"%>
        <label for="last_name">Last Name</label>

    </div>
      <div class="input-field">
        <i class="material-icons prefix">email</i>
        <%= f.email_field :email, :id=>"email", class:"validate"%>
        <label for="email" data-error="wrong" data-success="right">Email</label>
      </div>
      <div class="fiel">
        Fotograf√≠a: <%= f.file_field :cover %>
      </div>
        <br>

      <div class="input-field">
        <%= f.collection_select(:member_id, :gender, Member::MEMBER_GENDERS)%>
      </div>
      <div class="fiel">
        <%= f.submit "Registrar", class:"" %>
      </div>
<% end %>
    </div>
</div>